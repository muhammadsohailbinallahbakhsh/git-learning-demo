{
  "name": "git-learning-demo",
  "version": "1.0.0",
  "description": "A simple TypeScript project for learning Git",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon src/main.ts",
    "dev:watch": "concurrently \"tsc --watch\" \"nodemon dist/main.js\"",
