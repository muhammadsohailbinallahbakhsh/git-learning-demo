  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.0.0",
    "jest": "^28.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.0.0",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "lodash": "^4.17.21"
  }
}
